{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/manifest.webmanifest"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "jokes-cache",
      "urls": [
        "https://icanhazdadjoke.com/"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 5,
        "maxAge": "1d"
      }
    },
    {
      "name": "cats-cache",
      "urls": [
        "https://api.thecatapi.com/v1/images/search",
        "https://cdn2.thecatapi.com/images/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 1,
        "maxAge": "1d",
        "timeout": "5s"
      }
    }
  ]
}
